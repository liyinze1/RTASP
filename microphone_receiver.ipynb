    "from RTASP import *\n",
    "import time\n",
    "\n",
    "receiver = RTASP_receiver(print=False)\n",
    "\n",
    "sensor_addr = ('10.147.19.81', 23000)\n",
    "\n",
    "receiver.discover(sensor_addr)\n",
    "\n",
    "receiver.start(sensor_addr)\n",
    "\n",
    "time.sleep(10)\n",
    "\n",
    "receiver.stop(sensor_addr)\n",
    "\n",
    "print(receiver.end(('127.0.0.1', 23000)))\n"
